{
  "name": "phansible/phansible",
  "description": "Phansible - generate Vagrant + Ansible dev environments for PHP",
  "homepage": "http://phansible.com",
  "authors": [
    {
      "name": "Erika Heidi",
      "homepage": "http://erikaheidi.com"
    },
    {
      "name": "Marco \"Debo\" De Bortoli",
      "homepage": "https://debo.io"
    }
  ],
  "license": "MIT",
  "config": {
    "bin-dir": "bin"
   },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "symfony/flex": "^1.17",
    "symfony/http-foundation": "^5.3",
    "symfony/routing": "^5.3",
    "symfony/http-kernel": "^5.3",
    "symfony/event-dispatcher": "^5.3",
    "symfony/dependency-injection": "^5.3",
    "symfony/proxy-manager-bridge": "^5.3",
    "symfony/config": "^5.3",
    "symfony/yaml": "^5.3",
    "twig/markdown-extra": "^2.14",
    "knplabs/github-api": "^2.12",
    "php-http/guzzle6-adapter": "^1.1",
    "cache/filesystem-adapter": "^1.1",
    "league/commonmark": "^1.6",
    "doctrine/annotations": "^1.13",
    "ext-zip": "^1.15",
    "symfony/twig-bundle": "^5.3",
    "twig/extra-bundle": "^3.3",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5",
    "symfony/dotenv": "^5.3",
    "squizlabs/php_codesniffer": "^3.6",
    "symfony/console": "^5.3"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    }
  }
}
