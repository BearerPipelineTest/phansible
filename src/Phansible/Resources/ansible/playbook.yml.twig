---
- hosts: all
  sudo: true
  vars:
    web_server: {{ web_server }}
{% for key, value in playbook_vars %}
    {{ key }}: {{ value }}
{% endfor %}
  vars_files:
{% for file in playbook_files %}
    - {{ file }}
{% endfor %}
  roles:
{% for ansible_role in playbook_roles %}
    - {{ ansible_role }}
{% endfor %}
